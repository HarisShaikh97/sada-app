{
	"expo": {
		"name": "sada-app",
		"slug": "sada-app",
		"version": "1.0.0",
		"scheme": "sada-app-scheme",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/splash.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.harisshaikh.sadaapp"
		},
		"web": {
			"favicon": "./assets/icon.png"
		},
		"plugins": ["expo-router"],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "0ce13a5d-9753-4719-9136-cef7b8c5e4bd"
			}
		}
	}
}
